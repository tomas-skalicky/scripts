#!/usr/bin/env bash
#author      :Tomas Skalicky <skalicky.tomas@gmail.com>
#===============================================================================

#-------------------------------------------------------------------------------

openjdk_packages_to_install=(
    default-jdk
)

#-------------------------------------------------------------------------------

install_openjdk_packages() {
    aptget_install "${openjdk_packages_to_install[@]}"
}

#-------------------------------------------------------------------------------

main() {
    install_openjdk_packages
}

#-------------------------------------------------------------------------------

main

