#!/usr/bin/env bash
#author      :Tomas Skalicky <skalicky.tomas@gmail.com>
#===============================================================================

#-------------------------------------------------------------------------------

mysql_packages_to_install=(
    mysql-workbench
)

#-------------------------------------------------------------------------------

install_mysql_packages() {
    aptget_install "${mysql_packages_to_install[@]}"
}

#-------------------------------------------------------------------------------

main() {
    install_mysql_packages
}

#-------------------------------------------------------------------------------

main

